{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "Super Bookmark Desktop",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "chrome://favicon/"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "minimum_chrome_version": "59"
}
