{
    "version": "1.5.0",
    "manifest_version": 3,
    "name": "Super Bookmark Desktop",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks",
        "favicon",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "minimum_chrome_version": "104"
}
